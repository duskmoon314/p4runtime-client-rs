{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^prost", "^tonic"],
      "groupName": "prost and tonic crates"
    },
    {
      "matchPackagePatterns": ["^tokio"],
      "groupName": "tokio crates"
    }
  ]
}
